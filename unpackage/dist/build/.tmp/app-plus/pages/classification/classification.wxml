<view><view class="navSearch"><view class="ns-space"></view><view data-event-opts="{{[['tap',[['navTo',['index/search',0]]]]]}}" class="ns-main" bindtap="__e"><view class="nm-icon"><image src="/static/icon/icon_ss_sy.png" mode></image></view><view class="nm-inp"><input type="text" placeholder="请输入想要搜索的商品"/></view></view></view><view class="navSearchSpace"></view><view class="topTabSpace"></view><view class="topTab"><block wx:for="{{tabList}}" wx:for-item="t" wx:for-index="t_idx" wx:key="t_idx"><view data-event-opts="{{[['tap',[['tabChange',[t_idx]]]]]}}" class="{{['tt-item',current===t_idx?'active':'']}}" bindtap="__e">{{''+t.name+''}}</view></block></view><view class="slideMain"><block wx:for="{{getEachKindsInfos}}" wx:for-item="c" wx:for-index="c_idx" wx:key="c_idx"><view class="goodsAndComments"><view data-event-opts="{{[['tap',[['navTo',['index/postDetails','$0'],[[['getEachKindsInfos','',c_idx,'id']]]]]]]}}" class="postMain" bindtap="__e"><view class="pm-desc"><view class="pd-title">{{''+c.title+''}}</view><view class="pd-desc">{{''+c.details+''}}</view><view class="pd-show"><block wx:for="{{c.imgsArr}}" wx:for-item="i" wx:for-index="i_idx" wx:key="i_idx"><view class="ps-item"><image src="{{i}}" mode></image></view></block></view></view></view></view></block></view></view>